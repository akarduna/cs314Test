import requests
import json
f = open("student-main/tests/sprint2/test.json")

url = "https://black-bottle.cs.colostate.edu:31427/api/config"

data = json.load(f)
config = {"requestType" : "config"}
find = {
  "requestType"    : "find",
  "match"          : "dave",
  "limit"          : 10
}

x = requests.post(url, json = find, verify = False)

print(x.status_code)
print(x.text)
print(x.content)
